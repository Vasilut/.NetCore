@model IEnumerable<DartuContestHosted.Models.Rezultate>


<div class="panel panel-body panel-danger">
        <table class="table table-striped table-hover">
            <tr>
                <th>
                    ID
                </th>
                <th>
                    Nume
                </th>
                <th>
                    Prenume
                </th>
                <th>
                    Clasa
                </th>
                <th>
                    Scoala
                </th>
                <th>
                    Problema 1
                </th>
                <th>
                    Problema 2
                </th>
                <th>
                    Problema 3
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Nume)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Prenume)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Clasa)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Scoala)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.P1)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.P2)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.P2)
                    </td>
                </tr>
            }
        </table>
    </div>

<div>
    @Html.ActionLink("Back", "Index")
</div>

